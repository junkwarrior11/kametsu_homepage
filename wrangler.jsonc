{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "kametsu-homepage",
  "compatibility_date": "2026-02-17",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  // D1 Database - for local development, use --local flag
  // database_id will be set after creating production database
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "webapp-production",
      "database_id": "local-dev"
    }
  ]
}